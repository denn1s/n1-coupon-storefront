{
  "name": "tanstack-admin-template",
  "version": "1.0.0",
  "description": "A modern React admin template with TanStack Router, TanStack Query, DaisyUI, and Tailwind CSS",
  "author": "Your Name",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "tanstack-router",
    "tanstack-query",
    "react-query",
    "daisyui",
    "tailwind",
    "admin-template",
    "starter-template"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && vite build --mode development",
    "analyze": "ANALYZE=true npm run build",
    "typecheck": "tsc -b --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "fix": "npm run lint:fix && npm run format",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "serve": "vite build && vite preview --port 3000",
    "test": "vitest --run",
    "test-watch": "vitest",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.134.13",
    "@tanstack/react-router-devtools": "^1.134.13",
    "@tanstack/react-store": "^0.8.0",
    "@tanstack/store": "^0.8.0",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "@types/qrcode": "^1.5.6",
    "country-flag-icons": "^1.5.21",
    "daisyui": "^5.4.7",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.24",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.1",
    "graphql-tag": "^2.12.6",
    "init": "^0.1.2",
    "marked": "^17.0.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-querybuilder": "^8.11.2",
    "react-select": "^5.10.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@eslint/js": "^9.22.0",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@storybook/react": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@tanstack/eslint-plugin-query": "^5.90.7",
    "@tanstack/router-plugin": "^1.134.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "react-scan": "^0.3.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^10.0.6",
    "tailwindcss": "^4.1.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-federation": "^0.0.1",
    "vitest": "^3.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  }
}
