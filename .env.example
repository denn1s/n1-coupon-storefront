# Auth0 Configuration
# Get these values from your Auth0 dashboard (https://manage.auth0.com/)
VITE_AUTH_DOMAIN=your-tenant.auth0.com
VITE_AUTH_CLIENT_ID=your_auth0_client_id_here
VITE_AUTH_REDIRECT_URI=http://localhost:3000
VITE_AUTH_AUDIENCE=your_api_audience_here

# API Configuration
# Point to your backend API
VITE_API_HOST=http://localhost:8000
VITE_API_MOUNT=/api/v1

# Optional: Environment indicator
VITE_ENV=development
